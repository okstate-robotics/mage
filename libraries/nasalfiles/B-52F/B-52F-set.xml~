<!-- Steering -->
      <key n="26">
        <name>CTRL-z</name>
        <desc>Steer Right</desc>
        <repeatable type="bool">true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property>/controls/gear/steering-front-norm</property>
          <step type="double">0.001886792</step>
        </binding>
      </key>
      <key n="28">
        <name>CTRL-\</name>
        <desc>Steer Left</desc>
